begin
declare idP int;
INSERT INTO saude_fichamedica(id_fichamedica, id_pessoa, descricao) VALUES (default, id_pessoa, texto);
select max(id_pessoa) into idP FROM pessoa;
END


begin
declare idP int;
select max(id_pessoa) into idP FROM pessoa;
INSERT INTO saude_fichamedica(id_fichamedica, id_pessoa, descricao) VALUES (default, idP, descricao);

END